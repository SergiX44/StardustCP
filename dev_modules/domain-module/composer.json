{
    "name": "stardustcp/domain-module",
    "version": "1.0",
    "description": "Migrations module for the Web and DNS module",
    "keywords": [
        "stardustcp",
        "domain-module"
    ],
    "homepage": "https://github.com/stardustcp/domain-module",
    "license": "AGPL-3.0",
    "authors": [
        {
            "name": "Sergio Brighenti",
            "email": "sergio@brighenti.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "ext-json": "*",
        "thecodingmachine/safe": "^0.1.16"
    },
    "autoload": {
        "psr-4": {
            "Modules\\Domain\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Modules\\Domain\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Modules\\Domain\\DomainServiceProvider"
            ]
        }
    }
}
