{
	"name": "startdustcp/startdustcp",
	"version": "1.0",
	"type": "project",
	"description": "The StardustCP control panel.",
	"keywords": [
		"hosting",
		"control",
		"panel"
	],
	"license": "AGPL-3.0",
	"repositories": [
		{
			"type": "path",
			"url": "./dev_modules/web-module"
		},
		{
			"type": "path",
			"url": "./dev_modules/domain-module"
		},
		{
			"type": "path",
			"url": "./dev_modules/database-module"
		}
	],
	"require": {
		"php": "^7.2",
		"ext-json": "*",
		"ext-posix": "*",
		"fideloper/proxy": "^4.0",
		"laravel/framework": "^6.0",
		"laravel/helpers": "^1.1",
		"laravel/tinker": "^1.0",
		"stardustcp/database-module": "^1.0",
		"stardustcp/web-module": "^1.0",
		"symfony/process": "^4.3"
	},
	"require-dev": {
		"facade/ignition": "^1.4",
		"fzaninotto/faker": "^1.4",
		"mockery/mockery": "^1.0",
		"nunomaduro/collision": "^3.0",
		"phpunit/phpunit": "^8.0"
	},
	"config": {
		"optimize-autoloader": true,
		"preferred-install": "dist",
		"sort-packages": true
	},
	"extra": {
		"laravel": {
			"dont-discover": []
		}
	},
	"autoload": {
		"psr-4": {
			"Core\\": "app/"
		},
		"classmap": [
			"database/seeds",
			"database/factories"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/"
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"scripts": {
		"post-autoload-dump": [
			"Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
			"@php artisan package:discover --ansi"
		],
		"post-root-package-install": [
			"@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
		],
		"post-create-project-cmd": [
			"@php artisan key:generate --ansi"
		]
	}
}
